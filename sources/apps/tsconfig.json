{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
      "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react",
      "outDir": "./dist"
  },
    "baseUrl": "./",
    "paths": {
        "@libs/": [
            "libs/"
        ],
        "@libs/*": [
            "libs/*"
        ],
        "@apps/": [
            "apps/"
        ],
        "@apps/*": [
            "apps/*"
        ]
    },
  "include": [
    "src"
  ]
}
