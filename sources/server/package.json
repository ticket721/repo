{
    "name": "@ticket721sources/server",
    "version": "1.0.0",
    "description": "",
    "author": "",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
        "start": "nest start",
        "start:server:dev": "nest start server --watch",
        "start:server:debug": "nest start server --debug --watch",
        "start:server:prod": "node dist/main",
        "doc:serve": "compodoc -p compodoc-tsconfig.json --theme stripe --unitTestCoverage ./coverage/coverage-summary.json -s",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test:server": "ts-mocha -p apps/server/tsconfig.app.json --paths --timeout 10000 'apps/server/src/**/*.spec.ts'",
        "test:server:cov": "nyc --report-dir=coverage-server npm run test:server",
        "test:server:e2e": "env NODE_ENV=test-e2e ts-mocha -p apps/server/tsconfig.app.json --paths --timeout 30000 'apps/server/test/**/*.e2e-spec.ts'",
        "test:server:e2e:cov": "nyc --report-dir=coverage-server-e2e npm run test:server:e2e",
        "test:common": "ts-mocha -p libs/common/tsconfig.lib.json --paths --timeout 10000 'libs/common/src/**/*.spec.ts'",
        "test:common:cov": "nyc --report-dir=coverage-common npm run test:common",
        "test:common:e2e": "env NODE_ENV=test-e2e ts-mocha -p libs/common/tsconfig.lib.json --paths --timeout 30000 'libs/common/test/**/*.e2e-spec.ts'",
        "test:common:e2e:cov": "nyc --report-dir=coverage-common-e2e npm run test:common:e2e"
    },
    "dependencies": {
        "@iaminfinity/express-cassandra": "6.0.0",
        "@nestjs/bull": "0.0.1-beta.4",
        "@nestjs/common": "6.10.12",
        "@nestjs/core": "6.10.11",
        "@nestjs/jwt": "6.1.1",
        "@nestjs/passport": "6.1.1",
        "@nestjs/platform-express": "6.10.11",
        "@nestjs/swagger": "4.0.9",
        "@ticket721sources/global": "file:../global",
        "@ticket721sources/sdk": "file:../sdk",
        "bcrypt": "3.0.7",
        "bull": "3.12.1",
        "bull-board": "0.6.0",
        "chai": "4.2.0",
        "chai-as-promised": "7.1.1",
        "class-transformer": "0.2.3",
        "class-validator": "0.11.0",
        "dotenv": "8.2.0",
        "elasticsearch": "16.5.0",
        "git-branch": "2.0.1",
        "mocha": "6.2.2",
        "nest-schedule": "0.6.3",
        "passport": "0.4.1",
        "passport-jwt": "4.0.0",
        "passport-local": "1.0.0",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.0",
        "rxjs": "6.5.3",
        "swagger-ui-express": "4.1.2",
        "web3": "1.2.4",
        "winston": "3.2.1"
    },
    "devDependencies": {
        "@compodoc/compodoc": "^1.1.11",
        "@istanbuljs/nyc-config-typescript": "0.1.3",
        "@nestjs/cli": "^6.9.0",
        "@nestjs/schematics": "^6.7.0",
        "@nestjs/testing": "^6.7.1",
        "@types/bull": "^3.10.6",
        "@types/chai": "^4.2.7",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.7.5",
        "@types/passport-jwt": "^3.0.3",
        "@types/passport-local": "^1.0.33",
        "@types/supertest": "^2.0.8",
        "chai-shallow-deep-equal": "^1.4.6",
        "chai-subset": "^1.6.0",
        "dockerode": "^3.0.2",
        "fs-extra": "^8.1.0",
        "nyc": "^15.0.0",
        "prettier": "^1.18.2",
        "ts-loader": "^6.1.1",
        "ts-mocha": "^6.0.0",
        "ts-mockito": "^2.5.0",
        "ts-node": "^8.4.1",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^5.20.0",
        "typescript": "3.7.4"
    },
    "nyc": {
        "extends": "@istanbuljs/nyc-config-typescript",
        "all": true,
        "reporter": [
            "text",
            "lcov"
        ],
        "include": [
            "**/*.ts"
        ],
        "exclude": [
            "node_modules",
            "**/*.spec.ts",
            "**/*.module.ts",
            "dist",
            "test",
            "apps/server/test",
            "apps/server/src/main.ts",
            "apps/server/src/utils/HttpException.filter.ts",
            "apps/server/src/authentication/Jwt.strategy.ts",
            "libs/common/src/logger/*"
        ]
    }
}
