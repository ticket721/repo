{
    "name": "@ticket721sources/server",
    "version": "1.0.0",
    "description": "",
    "author": "",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build:server": "npm run prebuild && nest build server --tsc",
        "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
        "format:check": "prettier --check \"apps/**/*.ts\" \"libs/**/*.ts\"",
        "start": "nest start",
        "start:server:dev": "nest start server --watch",
        "start:server:debug": "nest start server --debug --watch",
        "start:server:prod": "node dist/main",
        "doc:serve": "compodoc -p compodoc-tsconfig.json --theme stripe -s",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test": "jest --expand --verbose",
        "test:cov": "npm run test -- --coverage",
        "test:server:e2e": "env NODE_ENV=test-e2e jest -c ./jest.server.e2e-config.js --expand --verbose --runInBand --forceExit",
        "test:server:e2e:cov": "npm run test:server:e2e -- --coverage"
    },
    "dependencies": {
        "@iaminfinity/express-cassandra": "6.0.0",
        "@nestjs/bull": "0.0.1-beta.4",
        "@nestjs/common": "6.10.14",
        "@nestjs/core": "6.10.14",
        "@nestjs/jwt": "6.1.1",
        "@nestjs/passport": "6.1.1",
        "@nestjs/platform-express": "6.11.3",
        "@nestjs/swagger": "4.1.10",
        "@ticket721sources/global": "file:../global",
        "@ticket721sources/sdk": "file:../sdk",
        "bcrypt": "3.0.7",
        "bull": "3.12.1",
        "bull-board": "0.6.0",
        "class-transformer": "0.2.3",
        "class-validator": "0.11.0",
        "dotenv": "8.2.0",
        "elasticsearch": "16.5.0",
        "email-templates": "6.1.1",
        "git-branch": "2.0.1",
        "nest-schedule": "0.6.4",
        "node-mailjet": "3.3.1",
        "passport": "0.4.1",
        "passport-jwt": "4.0.0",
        "passport-local": "1.0.0",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.0",
        "rxjs": "6.5.4",
        "swagger-ui-express": "4.1.2",
        "winston": "3.2.1"
    },
    "devDependencies": {
        "@compodoc/compodoc": "^1.1.11",
        "@nestjs/cli": "^6.9.0",
        "@nestjs/schematics": "^6.7.0",
        "@nestjs/testing": "^6.7.1",
        "@types/bull": "^3.10.6",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.1",
        "@types/jest": "24.0.25",
        "@types/passport-jwt": "^3.0.3",
        "@types/passport-local": "^1.0.33",
        "@types/supertest": "^2.0.8",
        "@types/node": "12.12.22",
        "dockerode": "^3.0.2",
        "fs-extra": "^8.1.0",
        "jest": "24.9.0",
        "prettier": "^1.18.2",
        "ts-jest": "24.3.0",
        "ts-loader": "^6.1.1",
        "ts-mockito": "^2.5.0",
        "ts-node": "^8.4.1",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^5.20.0",
        "typescript": "3.7.4"
    },
    "nyc": {
        "extends": "@istanbuljs/nyc-config-typescript",
        "all": true,
        "reporter": [
            "text",
            "lcov"
        ],
        "include": [
            "**/*.ts"
        ],
        "exclude": [
            "node_modules",
            "**/*.spec.ts",
            "**/*.module.ts",
            "dist",
            "test",
            "apps/server/test",
            "apps/server/src/main.ts",
            "apps/server/src/utils/HttpException.filter.ts",
            "apps/server/src/authentication/Jwt.strategy.ts",
            "libs/common/src/logger/*"
        ]
    }
}
