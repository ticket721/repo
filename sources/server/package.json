{
    "name": "@ticket721sources/server",
    "version": "1.0.0",
    "description": "",
    "author": "",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "doc:serve": "compodoc -p compodoc-tsconfig.json --theme stripe --unitTestCoverage ./coverage/coverage-summary.json -s",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test": "ts-mocha --timeout 10000 'src/**/*.spec.ts'",
        "test:cov": "nyc --reporter=lcov --reporter=json-summary npm run test",
        "test:e2e": "env NODE_ENV=test-e2e ts-mocha --timeout 30000 'test/**/*.e2e-spec.ts'",
        "test:e2e:cov": "nyc --report-dir=coverage-e2e npm run test:e2e"
    },
    "dependencies": {
        "@iaminfinity/express-cassandra": "6.0.0",
        "@nestjs/common": "6.10.11",
        "@nestjs/core": "6.10.11",
        "@nestjs/jwt": "6.1.1",
        "@nestjs/passport": "6.1.1",
        "@nestjs/platform-express": "6.10.11",
        "@nestjs/swagger": "4.0.9",
        "@ticket721sources/global": "1.0.0",
        "bcrypt": "3.0.7",
        "chai": "4.2.0",
        "chai-as-promised": "7.1.1",
        "class-transformer": "0.2.3",
        "class-validator": "0.11.0",
        "dotenv": "8.2.0",
        "elasticsearch": "16.5.0",
        "git-branch": "2.0.1",
        "mocha": "6.2.2",
        "passport": "0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "1.0.0",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.0",
        "rxjs": "6.5.3",
        "swagger-ui-express": "4.1.2",
        "winston": "3.2.1"
    },
    "devDependencies": {
        "@compodoc/compodoc": "^1.1.11",
        "@istanbuljs/nyc-config-typescript": "^0.1.3",
        "@nestjs/cli": "^6.9.0",
        "@nestjs/schematics": "^6.7.0",
        "@nestjs/testing": "^6.7.1",
        "@ticket721sources/sdk": "1.0.0",
        "@types/chai": "^4.2.7",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.7.5",
        "@types/passport-jwt": "^3.0.3",
        "@types/passport-local": "^1.0.33",
        "@types/supertest": "^2.0.8",
        "dockerode": "^3.0.2",
        "fs-extra": "^8.1.0",
        "nyc": "^14.1.1",
        "prettier": "^1.18.2",
        "ts-loader": "^6.1.1",
        "ts-mocha": "^6.0.0",
        "ts-mockito": "^2.5.0",
        "ts-node": "^8.4.1",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^5.20.0",
        "typescript": "3.7.3"
    },
    "nyc": {
        "extends": "@istanbuljs/nyc-config-typescript",
        "all": true,
        "reporter": [
            "text",
            "lcov"
        ],
        "include": [
            "**/*.ts"
        ],
        "exclude": [
            "node_modules",
            "src/**/*.spec.ts",
            "test/**/*.e2e-spec.ts",
            "dist",
            "src/main.ts",
            "test",
            "src/logger/*",
            "src/utils/HttpException.filter.ts"
        ]
    }
}
