{
    "collection": "@nestjs/schematics",
    "sourceRoot": "apps/server/src",
    "monorepo": true,
    "root": "apps/server",
    "compilerOptions": {
        "webpack": false,
        "tsConfigPath": "tsconfig.json"
    },
    "projects": {
        "server": {
            "type": "application",
            "root": "apps/server",
            "entryFile": "main",
            "sourceRoot": "apps/server/src",
            "compilerOptions": {
                "tsConfigPath": "apps/server/tsconfig.app.json"
            }
        },
        "worker": {
            "type": "application",
            "root": "apps/worker",
            "entryFile": "main",
            "sourceRoot": "apps/worker/src",
            "compilerOptions": {
                "tsConfigPath": "apps/worker/tsconfig.app.json"
            }
        },
        "common": {
            "type": "library",
            "root": "libs/common",
            "entryFile": "index",
            "sourceRoot": "libs/common/src",
            "compilerOptions": {
                "tsConfigPath": "libs/common/tsconfig.lib.json"
            }
        }
    }
}
