{
    "name": "@frontend/staff-app",
    "description": "Ticket721 Staff Application",
    "version": "0.0.1",
    "private": true,
    "dependencies": {
        "@capacitor/core": "2.0.1",
        "@frontend/core": "1.0.0",
        "@frontend/flib-react": "1.0.0",
        "@ionic/react": "5.0.7",
        "@ionic/react-router": "5.0.7",
        "@testing-library/jest-dom": "4.2.4",
        "@testing-library/react": "9.4.0",
        "@testing-library/user-event": "8.0.3",
        "@types/jest": "24.0.25",
        "@types/node": "12.12.24",
        "@types/react": "16.9.35",
        "@types/react-dom": "16.9.6",
        "@types/react-router": "5.1.5",
        "@types/react-router-dom": "5.1.4",
        "ethers": "4.0.45",
        "ionicons": "5.1.2",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-qr-reader": "2.2.1",
        "react-router": "5.1.2",
        "react-router-dom": "5.1.2",
        "redux": "4.0.5",
        "styled-components": "5.1.0",
        "typescript": "3.8.3",
        "serve": "11.3.2",
        "cordova-plugin-screen-orientation": "3.0.2",
        "@ionic-native/screen-orientation": "5.27.0",
        "@ionic-native/core": "5.27.0",
        "cordova-res": "0.15.1",
        "es6-promise-plugin": "4.2.2"
    },
    "scripts": {
        "start": "PORT=3002 react-scripts start",
        "build": "react-scripts build",
        "build:prod": "react-scripts build",
        "test": "react-scripts test",
        "lint": "tslint --project .",
        "inject_segment_key": "sed -i -e \"s/__REACT_APP_SEGMENT_API_KEY__/${REACT_APP_SEGMENT_API_KEY}/g\" ./build/index.html",
        "inject_env": "node env_injector.js > /tmp/index.html && cp /tmp/index.html ./build/index.html",
        "inject_env_and_serve": "yarn inject_env && yarn inject_segment_key && serve -l 5000 ./build",
        "resources": "cordova-res ios && cordova-res android && node scripts/resources.js",
        "@lerna/postinstall": "echo '@frontend/staff-app has no postinstall'",
        "@lerna/postclean": "echo '@frontend/staff-app has no postclean'"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@capacitor/cli": "2.0.1",
        "@types/react-qr-reader": "2.1.3",
        "@react-workspaces/react-scripts": "3.4.0-alpha-01",
        "react-scripts": "3.4.1",
        "tslint": "6.0.0"
    }
}
