{
    "headers": [
        {
            "source" : "**/*.@(js|css|otf|woff|eot|ttf|woff2|svg|html)",
            "headers" : [{
                "key" : "Cache-Control",
                "value" : "no-cache"
            }]
        },
        {
            "source" : "static/**/*.@(js|css|otf|woff|eot|ttf|woff2|svg)",
            "headers" : [{
                "key" : "Cache-Control",
                "value" : "max-age=31536000"
            }]
        }
    ],
    "rewrites": [
        {
            "source": "/!(.well-known)*/**",
            "destination": "/index.html"
        },
        {
            "source": "/!(.well-known)*",
            "destination": "/index.html"
        }
    ],
    "directoryListing": false
}
