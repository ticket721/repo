{
    "name": "@frontend/t721-app",
    "description": "Ticket721 User Application",
    "version": "0.0.1",
    "private": true,
    "dependencies": {
        "@capacitor/android": "2.0.1",
        "@capacitor/core": "2.0.1",
        "@capacitor/ios": "2.0.1",
        "@frontend/core": "1.0.0",
        "@frontend/flib-react": "1.0.0",
        "@ionic/react": "5.0.7",
        "@ionic/react-router": "5.0.7",
        "@testing-library/jest-dom": "4.2.4",
        "@testing-library/react": "9.4.0",
        "@testing-library/user-event": "8.0.3",
        "@types/jest": "24.0.25",
        "@types/node": "12.12.24",
        "@types/react": "16.9.35",
        "@types/react-dom": "16.9.6",
        "@types/react-router": "5.1.5",
        "@types/react-router-dom": "5.1.4",
        "@types/qrcode.react": "1.0.1",
        "connected-react-router": "6.8.0",
        "ethers": "4.0.45",
        "ionicons": "5.0.0",
        "react": "16.13.1",
        "formik": "2.1.4",
        "quick-score": "0.0.8",
        "qrcode.react": "1.0.0",
        "react-dom": "16.13.1",
        "react-redux": "7.2.0",
        "react-router": "5.1.2",
        "react-router-dom": "5.1.2",
        "styled-components": "5.1.0",
        "typescript": "3.8.3",
        "@hapi/joi": "17.1.0",
        "react-responsive": "8.1.0",
        "@types/react-responsive": "8.0.2",
        "@embarklabs/subspace": "2.0.3",
        "@embarklabs/subspace-react": "1.0.4",
        "react-i18next": "11.3.3",
        "rxjs": "6.5.5",
        "@capacitor/cli": "2.0.1",
        "react-scripts": "3.4.1",
        "tslint": "6.0.0",
        "react-countdown": "2.2.1",
        "@stripe/react-stripe-js": "1.1.2",
        "@stripe/stripe-js": "1.8.0",
        "serve": "11.3.2",
        "cordova-plugin-screen-orientation": "3.0.2",
        "@ionic-native/screen-orientation": "5.27.0",
        "@ionic-native/core": "5.27.0",
        "capacitor-stripe": "1.0.0-beta.4",
        "capacitor-plugin-ios-swipe-back": "silviokennecke/capacitor-plugin-ios-swipe-back#master",
        "cordova-res": "0.15.1",
        "es6-promise-plugin": "4.2.2",
        "cordova-plugin-brightness": "0.2.0",
        "@ionic-native/brightness": "5.27.0",
        "@jairemix/capacitor-segment": "1.0.3"
    },
    "resolutions": {
        "react-scripts/**/jest": "24.9.0"
    },
    "scripts": {
        "inject_env": "node env_injector.js > /tmp/index.html && cp /tmp/index.html ./build/index.html",
        "start": "PORT=8100 react-scripts start",
        "build": "react-scripts --max_old_space_size=4096 build",
        "build:prod": "react-scripts build",
        "test": "react-scripts test",
        "lint": "tslint --project .",
        "inject_segment_key": "sed -i -e \"s/__REACT_APP_SEGMENT_API_KEY__/${REACT_APP_SEGMENT_API_KEY}/g\" ./build/index.html",
        "inject_env_and_serve": "yarn inject_env && yarn inject_segment_key && serve -l 5000 ./build",
        "ionic:build:after": "yarn inject_env && yarn inject_segment_key",
        "resources": "cordova-res ios && cordova-res android && node scripts/resources.js",
        "@lerna/postinstall": "echo '@frontend/t721-app has no postinstall'",
        "@lerna/postclean": "echo '@frontend/t721-app has no postclean'"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
