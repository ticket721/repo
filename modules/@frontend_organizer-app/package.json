{
    "name": "@frontend/organizer-app",
    "description": "Ticket721 Organizer Application.",
    "version": "0.0.1",
    "private": true,
    "dependencies": {
        "@capacitor/android": "2.0.1",
        "@capacitor/core": "2.0.1",
        "@capacitor/ios": "2.0.1",
        "@frontend/core": "1.0.0",
        "@frontend/flib-react": "1.0.0",
        "@ionic/react": "5.0.7",
        "@ionic/react-router": "5.0.7",
        "lodash": "4.17.15",
        "@material-ui/core": "4.9.11",
        "@material-ui/icons": "4.9.1",
        "@testing-library/jest-dom": "4.2.4",
        "@testing-library/react": "9.4.0",
        "@testing-library/user-event": "8.0.3",
        "@types/jest": "24.0.25",
        "@types/material-ui": "0.21.7",
        "@types/node": "12.12.24",
        "@types/react": "16.9.35",
        "@types/react-dom": "16.9.8",
        "@types/react-router": "5.1.5",
        "@types/react-router-dom": "5.1.4",
        "@types/yup": "0.28.0",
        "colorthief": "2.3.0",
        "connected-react-router": "6.8.0",
        "formik": "2.1.4",
        "formik-persist": "1.1.0",
        "ionicons": "5.0.0",
        "node-vibrant": "3.1.2",
        "react": "16.13.1",
        "react-color": "2.18.1",
        "react-dom": "16.13.1",
        "react-google-places-autocomplete": "2.0.7",
        "react-redux": "7.2.0",
        "react-router": "5.1.2",
        "react-router-dom": "5.1.2",
        "react-select": "3.1.0",
        "redux": "4.0.5",
        "styled-components": "5.1.0",
        "typescript": "3.8.3",
        "yup": "0.28.4",
        "@embarklabs/subspace": "2.0.3",
        "@embarklabs/subspace-react": "1.0.4",
        "rxjs": "6.5.5",
        "@capacitor/cli": "2.0.1",
        "react-scripts": "3.4.1",
        "tslint": "6.0.0",
        "serve": "11.3.2"
    },
    "resolutions": {
        "react-scripts/**/jest": "24.9.0"
    },
    "scripts": {
        "inject_env": "node env_injector.js > /tmp/index.html && cp /tmp/index.html ./build/index.html",
        "start": "REACT_APP_ENV=dev PORT=3001 react-scripts start",
        "build": "react-scripts --max_old_space_size=4096 build",
        "inject_segment_key": "sed -i -e \"s/__REACT_APP_SEGMENT_API_KEY__/${REACT_APP_SEGMENT_API_KEY}/g\" ./build/index.html",
        "build:prod": "react-scripts build",
        "test": "react-scripts test",
        "lint": "tslint --project .",
        "inject_env_and_serve": "yarn inject_env && yarn inject_segment_key && serve -l 5000 ./build",
        "@lerna/postinstall": "echo '@frontend/organizer-app has no postinstall'",
        "@lerna/postclean": "echo '@frontend/organizer-app has no postclean'"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
