{
    "name": "@backend/nest",
    "private": true,
    "version": "1.0.0",
    "description": "Nest applications of the Ticket721 Platform",
    "author": "",
    "license": "MIT",
    "scripts": {
        "build:server": "nest build server",
        "build": "rimraf dist && yarn build:server",
        "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
        "format:check": "prettier --check \"apps/**/*.ts\" \"libs/**/*.ts\"",
        "start": "nest start",
        "start:server:dev": "nest start server --watch",
        "start:server:debug": "nest start server --debug --watch",
        "start:server:prod": "node dist/main",
        "doc:serve": "compodoc -p compodoc-tsconfig.json --theme stripe -s",
        "doc:coverage": "compodoc -p compodoc-tsconfig.json --coverageTest --coverageMinimumPerFile 100 --coverageTestShowOnlyFailed",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test": "jest --expand --verbose --testTimeout=10000",
        "test:cov": "yarn test --coverage",
        "test:server:e2e": "env NODE_ENV=test-e2e jest -c ./jest.server.e2e-config.js --expand --verbose --forceExit",
        "test:server:e2e:cov": "yarn test:server:e2e --coverage",
        "@lerna/postinstall": "echo '@backend/nest has no postinstall'",
        "@lerna/postclean": "echo '@backend/nest has no postclean'"
    },
    "dependencies": {
        "node-zip": "1.1.1",
        "handlebars-pdf": "1.0.1",
        "@common/geoloc": "1.0.0",
        "@common/global": "1.0.0",
        "@common/sdk": "1.0.0",
        "@hapi/joi": "17.1.0",
        "@iaminfinity/express-cassandra": "6.0.0",
        "@nestjs/cli": "7.0.0",
        "@nestjs/common": "6.11.8",
        "@nestjs/core": "6.11.8",
        "@nestjs/jwt": "6.1.2",
        "@nestjs/passport": "6.2.0",
        "@nestjs/platform-express": "6.11.8",
        "@nestjs/schematics": "6.7.0",
        "@nestjs/swagger": "4.7.16",
        "@nestjs/testing": "6.7.1",
        "@ticket721/e712": "file:../@public_e712",
        "@types/aws-sdk": "^2.7.0",
        "@types/bull": "3.10.6",
        "@types/dotenv": "8.2.0",
        "@types/express": "4.17.1",
        "@types/hapi__joi": "16.0.12",
        "@types/jest": "25.1.3",
        "@types/node": "13.7.7",
        "@types/passport-jwt": "3.0.3",
        "@types/passport-local": "1.0.33",
        "aws-sdk": "^2.758.0",
        "google-auth-library": "6.1.3",
        "axios": "0.19.2",
        "bcrypt": "4.0.0",
        "bignumber.js": "9.0.0",
        "binance-api-node": "0.9.15",
        "class-transformer": "0.2.3",
        "class-validator": "0.11.0",
        "decimal.js": "10.2.0",
        "dosojin": "file:../@public_dosojin",
        "dotenv": "8.2.0",
        "elasticsearch": "16.6.0",
        "email-templates": "7.0.4",
        "ethereum-address": "0.0.4",
        "ethers": "4.0.45",
        "fs-extra": "8.1.0",
        "lodash": "4.17.15",
        "nest-schedule": "0.6.4",
        "node-mailjet": "3.3.1",
        "parse-duration": "0.4.4",
        "passport": "0.4.1",
        "passport-jwt": "4.0.0",
        "passport-local": "1.0.0",
        "passport-custom": "1.1.1",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "6.5.4",
        "stripe": "8.12.0",
        "swagger-ui-express": "4.1.3",
        "ts-jest": "25.2.1",
        "ts-loader": "6.1.1",
        "typescript": "3.8.3",
        "web3": "1.2.6",
        "winston": "3.2.1"
    },
    "devDependencies": {
        "@compodoc/compodoc": "1.1.11",
        "@types/jest": "25.1.3",
        "@types/supertest": "2.0.8",
        "form-data": "3.0.0",
        "jest": "25.1.0",
        "jimp": "0.10.0",
        "prettier": "1.19.1",
        "ts-mockito": "2.5.0",
        "ts-node": "8.4.1",
        "tsconfig-paths": "3.9.0",
        "tslint": "6.0.0"
    },
    "nyc": {
        "extends": "@istanbuljs/nyc-config-typescript",
        "all": true,
        "reporter": [
            "text",
            "lcov"
        ],
        "include": [
            "**/*.ts"
        ],
        "exclude": [
            "node_modules",
            "**/*.spec.ts",
            "**/*.module.ts",
            "dist",
            "test",
            "apps/server/test",
            "apps/server/src/main.ts",
            "apps/server/src/utils/HttpException.filter.ts",
            "apps/server/src/authentication/Jwt.strategy.ts",
            "libs/common/src/logger/*"
        ]
    }
}
