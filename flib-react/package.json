    "ts-node": "8.6.2",
    "tslint": "6.0.0",
    "typescript": "3.7.5",
    "watch": "1.0.2",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/setupTests.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
