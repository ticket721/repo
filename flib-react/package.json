{
  "name": "@frontend/flib-react",
  "version": "1.0.0",
  "description": "",
  "source": "src/index.ts",
  "main": "./lib/src/index.js",
  "types": "./lib/index.d.ts",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "build": "rm -rf lib && tsc && echo \"Components compiled !\" && mkdir -p lib/assets && cp -r static/assets/fonts lib/assets/fonts && yarn gen-icons",
    "watch": "watch 'yarn build && echo \"Done !\"' src",
    "gen-icons": "rm -rf static/t721-icons && node scripts/svgtofont.js",
    "storybook": "start-storybook -p 6007 -s ./static",
    "build-storybook": "build-storybook -c .storybook -o dist -s ./static",
    "test": "jest --coverage",
    "test:update-snapshots": "jest --coverage -u",
    "@lerna/postinstall": "yarn build",
    "@lerna/postclean": "rm -rf lib && rm -rf static/t721-icons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ticket721/flib.git"
  },
  "author": "Iulian Rotaru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ticket721/flib/issues"
  },
  "homepage": "https://github.com/ticket721/flib#readme",
  "dependencies": {
    "@egjs/react-flicking": "3.2.2",
