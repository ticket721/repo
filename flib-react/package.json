    "@types/react-test-renderer": "16.9.2",
    "@types/webpack": "4.41.3",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "8.0.6",
    "conventional-changelog-cli": "2.0.31",
    "cpx": "1.5.0",
